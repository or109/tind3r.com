{
  "name": "react-hot-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "scripts": {
    "start": "node server.js",
    "heroku-postbuild": "npm run build",
    "lint": "eslint src",
    "prebuild": "",
    "build": "cross-env NODE_ENV=production webpack -p --config webpack.config.prod.js",
    "server": "NODE_ENV=production PORT=3000 node server.js",
    "server-dev": "cross-env NODE_ENV=development, PORT=3000 node server.js",
    "styleguide-server": "styleguidist server",
    "styleguide-build": "styleguidist build",
    "json-server": "json-server ./src/config/json-server.js --routes ./routes.json -p 3001"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-hot-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Dan Abramov <dan.abramov@me.com> (http://github.com/gaearon)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaearon/react-hot-boilerplate/issues"
  },
  "homepage": "https://github.com/gaearon/react-hot-boilerplate",
